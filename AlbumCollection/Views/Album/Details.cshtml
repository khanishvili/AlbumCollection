@model AlbumCollection.Models.Album
@{
    ViewData["Title"] = "Details";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/style/songs.css" rel="stylesheet" />


</head>
<body>
    <main>
        <section class="list">
            <h2>@Model.Artist_Name</h2>
            <ul class="songs" id="sevenfold">
                @foreach (var song in Model.Songs)
                {
                    <li>
                        <h3>@song.Song_Name</h3>
                        <h2>@song.SongId</h2>
                    </li>
                }
            </ul>
        </section>
    </main>
    <section>
        <h1><a href="/Song/Create">Add_Song</a></h1>
    </section>
</body>
</html>