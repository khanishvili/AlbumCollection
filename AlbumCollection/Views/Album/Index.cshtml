@model IEnumerable<AlbumCollection.Models.Album>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Index";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Style/Album.css" rel="stylesheet" />
</head>
<body>

    <h2>HI You are IN Album's Index Page </h2>

    @foreach (var album in Model)
    {
        <a href="/Album/Details/@album.AlbumId"> <h1>@album.Name</h1></a>
        <h2>@album.Artist_Name</h2>
    }






</body>
</html>